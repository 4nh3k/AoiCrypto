{
  "expo": {
    "name": "AoiCrypto",
    "slug": "AoiCrypto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Coinmoney.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/loading.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Coinmoney.png",
        "backgroundColor": "#2F66F6"
      },
      "package": "com.aoicrypto",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/figtree/Figtree_400Regular.ttf"
          ]
        }
      ],
      "expo-asset",
      "./queries.js",
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "669d0728-55bd-44f3-8230-679c8f535060"
      }
    }
  }
}
